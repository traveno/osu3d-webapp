{
	"name": "osu3d-webapp",
	"version": "1.2.0",
	"private": true,
	"scripts": {
		"dev": "NODE_ENV=development vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"update-types": "source .env && npx supabase gen types typescript --local > src/lib/types/supabase.ts",
		"db:pull": "dotenv -e .env npx prisma db pull",
		"dump-remote": "./supabase/dump-remote.sh",
		"reset-remote": "./supabase/reset-remote.sh",
		"types-remote": "./supabase/gen-types-remote.sh",
		"push-remote": "./supabase/push-remote.sh"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.25.2",
		"@tailwindcss/typography": "^0.5.10",
		"autoprefixer": "^10.4.16",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"supabase": "^1.100.1",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-floating-ui": "^1.5.3",
		"svelte-heros-v2": "^0.10.12",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.11"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.10.3",
		"@supabase/auth-ui-shared": "^0.1.7",
		"@supabase/auth-ui-svelte": "^0.2.6",
		"@supabase/supabase-js": "^2.38.0",
		"daisyui": "^3.9.2",
		"date-fns": "^2.30.0",
		"svelte-french-toast": "^1.2.0",
		"theme-change": "^2.5.0"
	}
}
